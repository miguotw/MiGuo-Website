---
layout: null
---
[
  {% assign items = site.posts | concat: site.projects %}
  {% for item in items %}
    {
      "title"  : "{{ item.title | escape }}",
      "url"    : "{{ site.baseurl }}{{ item.url }}",
      "content" : "{{ item.content | strip_html | strip_newlines | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]